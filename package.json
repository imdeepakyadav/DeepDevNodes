{
  "name": "deepdevnodes",
  "version": "1.0.0",
  "description": "A comprehensive REST API providing access to various data sources including financial data, space information, GitHub statistics, and more. Built with Node.js and Express.js for high performance and scalability.",
  "type": "module",
  "main": "src/server.js",
  "homepage": "https://github.com/imdeepakyadav/deepdevnodes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imdeepakyadav/deepdevnodes.git"
  },
  "bugs": {
    "url": "https://github.com/imdeepakyadav/deepdevnodes/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/imdeepakyadav"
  },
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "debug": "nodemon --inspect src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "format:check": "prettier --check src/**/*.js",
    "build": "echo 'Building deepdevnodes API for deployment...'",
    "vercel-build": "npm run lint",
    "update-data": "node -e \"import('./src/dataUpdater.js').then(m => m.manualUpdate())\"",
    "migrate-data": "node src/migrateData.js",
    "docker:build": "docker build -t deepdevnodes .",
    "docker:run": "docker run -p 3000:3000 deepdevnodes",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docs:serve": "npm run start",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "api",
    "rest",
    "express",
    "nodejs",
    "data-aggregation",
    "financial-data",
    "space-data",
    "github-api",
    "open-source",
    "backend",
    "web-development",
    "microservice",
    "api-hub"
  ],
  "author": "Deepak Yadav <imdeepakyadav@example.com>",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "dependencies": {
    "axios": "^1.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "mongodb": "^6.19.0",
    "mongoose": "^8.18.1",
    "morgan": "^1.10.0",
    "node-cron": "^4.2.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.5",
    "semantic-release": "^23.0.2",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es2022": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "off"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 80,
    "tabWidth": 2
  },
  "files": [
    "src/",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "contributors": [
    {
      "name": "Deepak Yadav",
      "email": "imdeepakyadav@example.com",
      "url": "https://github.com/imdeepakyadav"
    }
  ]
}
